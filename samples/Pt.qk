class Pt(x: Int, y: Int) {
	this.x : Int = x;
    this.y : Int = y;

	def _get_x(): Int {
		return this.x; 
	} 

	def _get_y(): Int {
		return this.y;
	}

	/*
	def translate(dx: Int, dy: Int): Nothing {
		this.x = this.x + dx;
		this.y = this.y + dy;
	}
	*/

    def PRINT(): Nothing {
       "( ".PRINT(); 
       this.x.PRINT(); 
       ", ".PRINT();
       ")".PRINT(); 
     }

	def PLUS(other: Pt): Pt {
		return Pt(this.x + other.x, this.y + other.y); 
	}
}

